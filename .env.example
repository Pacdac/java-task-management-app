# Environment Variables Example File
# Copy this file to .env and replace the placeholder values with your actual secrets

# JWT Configuration
JWT_SECRET=replace_with_secure_random_string_at_least_32_chars
JWT_EXPIRATION=86400000

# Database Configuration
POSTGRES_DB=taskdb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=replace_with_secure_database_password

# Spring Profiles
SPRING_PROFILES_ACTIVE=dev
